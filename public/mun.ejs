                            <input type="email" class="form-control" id="email" placeholder="Enter email" value='<%- user.email %>'
                                required>
                        </div>

                        <div class="form-group">
                            <label for="contact_mun" class="control-label">Contact Number*</label>
                            <input type="number" class="form-control" id="contact_mun" placeholder="Enter Contact Number"
                                value="<%= user.phoneNumber %>" required>
                        </div>

                        <div class="form-group">
                            <label for="college">Institute Name*</label>
                            <input type="text" class="form-control" id="college" placeholder="Enter Insitute Name"
                                value='<%- user.college %>' required>
                        </div>

                        <div class="form-group">
                            <label for="city">City*</label>
                            <input type="text" class="form-control" id="city" placeholder="Enter City" value='<%- user.city %>'
                                required>
                        </div>

                        <fieldset class="form-group">
                            <label>Choose Delegation*</label>
                            <br>
                            <div class="form-check  form-check-inline">
                                <label class="form-check-label">
                                    <input type="radio" class="form-check-input" name="typeRadio" id="ip" value="Delegates">
                                    Delegates
                                </label>
                            </div>
                            <div class="form-check  form-check-inline">
                                <label class="form-check-label">
                                    <input type="radio" class="form-check-input" name="typeRadio" id="del" value="IP">
                                    International Press
                                </label>
                            </div>


                        </fieldset>

                        <div id="hidden-matter">

                            <div class="form-group">
                                <label for="committee">Allotted Committee*</label>
                                <select class="form-control" id="committee" required>
                                </select>
                            </div>

                            <div class="form-group">
                                <label for="portfolio">Allotted Portfolio</label>
                                <input type="text" class="form-control" id="portfolio" placeholder="Enter Portfolio">
                            </div>

                            <div class="form-group">
                                <label for="referral">Refferal Code</label>
                                <input type="text" class="form-control" id="referral" placeholder="Enter Referral Code">
                            </div>

                            <div class="form-group">
                                <label for="accomodation">Accomodation*</label>
                                <select class="form-control" id="accomodation" required>
                                    <option value="Yes">Yes</option>
                                    <option value="No">No</option>
                                </select>
                            </div>

                            <div class="form-group align-center">
                                <label id="amount">Amount</label>

                            </div>
                            <div class="form-group">

                                <input type="hidden" class="form-control" id="mEmail" value="<%- user.email %>">
                            </div>

                            <div class="align-center">
                                <button type="submit" class="btn btn-dark btn--animated" id="submit-button">Pay</button>
                            </div>

                        </div>

                    </form>




                </div>
            </div>
        </div>
    </div>
    <% } %>



